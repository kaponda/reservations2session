{% extends 'base.html.twig' %}

{% block title %}Liste des types{% endblock %}

{% block body %}
    <h1>Liste des {{ resource }}</h1>

    <ul>
    {% for type in types %}
        <li><a href="{{ path('types_show', { id: type.id }) }}">{{ type.type }}</a></li>
    {% endfor %}
    </ul>
{% endblock %}

